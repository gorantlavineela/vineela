    "#python programming for printing the fibonacci series upto nth term\n",
    "n=int(input(\"Enter the Number\"))\n",
    "def fibonacci(n):\n",
    "    if(n==1):\n",
    "        return 0\n",
    "    elif(n==2):\n",
    "        return 1\n",
    "    else:\n",
    "        return fibonacci(n-1)+fibonacci(n-2)\n",
    "\n",
    "print(fibonacci(n))"
   ]
